import java.util.Scanner;

public class cubenumber {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number:");
        int n=sc.nextInt();
        int cube=n*n*n;
        System.out.println("cube number ="+cube);
        sc.close();
    }
}

output:
enter the number:
2
cube number =8
